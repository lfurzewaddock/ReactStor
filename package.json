{
  "name": "reactstor",
  "version": "0.0.1",
  "description": "React Redux e-Commerce",
  "main": "index.js",
  "scripts": {
    "testBuildBrowser": "node_modules/.bin/webpack --mode development --config webpack/test.browser",
    "testBuildNode": "node_modules/.bin/webpack --mode development --config webpack/test.node",
    "testNodeBundle": "npm run testBuildNode && node_modules/.bin/tape --require mock-css-modules --require jsdom-global/register dist/jsdom.environment.js dist/app.bundle.js | node_modules/.bin/tap-spec",
    "test": "NODE_ENV=development node_modules/.bin/tape --require @babel/register --require mock-css-modules test/jsdom.environment.js test/unit/{,**/}/*.test.js | node_modules/.bin/tap-spec",
    "testStart": "node_modules/.bin/webpack-dev-server --mode development --config webpack/test.browser --open",
    "testem": "node_modules/.bin/testem -f testem.node.json",
    "testIntegration": "NODE_ENV=development node_modules/.bin/tape --require @babel/register --require mock-css-modules --require jsdom-global/register test/jsdom.environment.js test/integration/{,**/}/*.test.js | node_modules/.bin/tap-spec",
    "debug": "NODE_ENV=debug node --nolazy --inspect-brk=5858 node_modules/.bin/tape --require @babel/register --require mock-css-modules test/jsdom.environment.js test/unit/{,**/}/*.test.js",
    "devBuild": "node_modules/.bin/webpack --mode development --config webpack/dev",
    "devWatch": "npm run devBuild -- --watch",
    "devStart": "node_modules/.bin/webpack-dev-server --mode development --config webpack/dev --open",
    "prodBuild": "node_modules/.bin/webpack --mode production --config webpack/prod",
    "lint": "node_modules/.bin/prettier-eslint \"*/**/*.js\" --log-level info",
    "lintFix": "node_modules/.bin/prettier-eslint  \"*/**/*.js\" --write --log-level warn",
    "coverage": "NODE_ENV=development node_modules/nyc/bin/nyc.js --require esm npm test",
    "coverReport": "npm run coverage && node_modules/nyc/bin/nyc.js report --reporter=html && ./bin/opn-cli coverage/index.html -- chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:lfurzewaddock/ReactStor.git"
  },
  "keywords": [
    "e-commerce",
    "ecommerce",
    "store",
    "shop",
    "react",
    "redux",
    "node"
  ],
  "author": "Leo Furze-Waddock <leo@furze-waddock.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lfurzewaddock/ReactStor/issues"
  },
  "homepage": "https://github.com/lfurzewaddock/ReactStor#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-umd": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-tape-runner": "^3.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "esm": "^3.0.84",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.4.4",
    "minimist": "^1.2.0",
    "mock-css-modules": "^1.0.0",
    "node-sass": "^4.9.4",
    "nyc": "^11.9.0",
    "opn": "^5.4.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.1.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-dev-utils": "^6.0.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "testem": "^2.13.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
